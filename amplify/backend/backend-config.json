{
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "type": "cicd"
    }
  },
  "auth": {
    "cafeshares": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false,
      "frontendAuthConfig": {
        "loginMechanisms": ["PREFERRED_USERNAME"],
        "signupAttributes": ["EMAIL"],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": "12",
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_UPPERCASE",
            "REQUIRES_NUMBERS"
          ]
        },
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": ["SMS", "TOTP"],
        "verificationMechanisms": ["EMAIL"]
      }
    }
  },
  "function": {
    "AdminQueriesbe6deefd": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "cafeshares",
          "attributes": ["UserPoolId"]
        }
      ]
    }
  },
  "api": {
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "authorizationType": "AMAZON_COGNITO_USER_POOLS",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "cafeshares",
          "attributes": ["UserPoolId"]
        },
        {
          "category": "function",
          "resourceName": "AdminQueriesbe6deefd",
          "attributes": ["Arn", "Name"]
        }
      ]
    },
    "CafeShares": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authcafeshares"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "API_KEY",
              "apiKeyConfig": {
                "apiKeyExpirationDays": 7,
                "apiKeyExpirationDate": "2021-12-27T16:18:23.749Z",
                "description": "CafeShares GraphQL backend API"
              }
            }
          ]
        }
      }
    }
  },
  "storage": {
    "cafesharesimagestorage": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  }
}
